{
  "name": "runme",
  "version": "0.1.0-edge.8",
  "publisher": "stateful",
  "displayName": "Runme",
  "description": "Run your README.md notebook style.",
  "author": "Sebastian Tiedtke <sebastian@stateful.com>",
  "contributors": [
    "Christian Bromann <christian@stateful.com>"
  ],
  "license": "Apache-2.0",
  "homepage": "https://runme.dev",
  "repository": {
    "type": "git",
    "url": "git://github.com/stateful/vscode-runme.git"
  },
  "bugs": {
    "url": "https://github.com/stateful/vscode-runme/issues"
  },
  "private": true,
  "engines": {
    "vscode": "^1.71.0"
  },
  "keywords": [
    "notebookRenderer"
  ],
  "badges": [
    {
      "url": "https://img.shields.io/badge/Marquee-Join%20our%20Community-blue?logo=discord&color=F62459",
      "href": "https://discord.gg/BQm8zRCBUY",
      "description": "Join us on Discord"
    }
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onNotebook:runme"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [],
    "notebooks": [
      {
        "type": "runme",
        "displayName": "Run your README.md",
        "selector": [
          {
            "filenamePattern": "*.md"
          },
          {
            "filenamePattern": "*.mdi"
          }
        ]
      }
    ],
    "notebookRenderer": [
      {
        "id": "runme-shell-renderer",
        "entrypoint": "./out/client.js",
        "displayName": "runme shell stdout",
        "mimeTypes": [
          "stateful.runme/shell-stdout"
        ]
      },
      {
        "id": "runme-vercel-renderer",
        "entrypoint": "./out/client.js",
        "displayName": "runme vercel stdout",
        "mimeTypes": [
          "stateful.runme/vercel-stdout"
        ]
      },
      {
        "id": "runme-html-renderer",
        "entrypoint": "./out/client.js",
        "displayName": "runme html stdout",
        "mimeTypes": [
          "stateful.runme/html-stdout"
        ]
      }
    ],
    "configuration": [
      {
        "title": "Runme",
        "properties": {
          "runme.shell.interactive": {
            "type": "boolean",
            "scope": "machine",
            "default": true,
            "markdownDescription": "If set to `true` all shell scripts are run as interactive VS Code tasks, if set to `false` they are executed within the kernel and output is set in resulting cell."
          }
        }
      }
    ]
  },
  "scripts": {
    "build": "run-s build:dev",
    "build:dev": "webpack --mode development",
    "build:prod": "webpack --mode production",
    "bundle": "vsce package --pre-release --yarn --no-git-tag-version --no-update-package-json -o \"./runme-$npm_package_version.vsix\"",
    "esmify": "echo '{ \"type\": \"module\" }' >> ./out/package.json",
    "test": "run-s test:*",
    "test:lint": "eslint src --ext ts",
    "watch": "npm run build:dev -- --watch"
  },
  "devDependencies": {
    "@types/node": "^18.7.19",
    "@types/vscode": "^1.71.0",
    "@types/vscode-notebook-renderer": "^1.72.0",
    "@types/webpack-env": "^1.18.0",
    "@types/yargs": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.24.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-unicorn": "^43.0.2",
    "eslint-plugin-unused-imports": "^2.0.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "npm-run-all": "^4.1.5",
    "semver": "^7.3.7",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3",
    "vsce": "^2.11.0",
    "vscode-notebook-error-overlay": "^1.0.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.8",
    "@vercel/client": "^12.2.7",
    "@vitejs/plugin-react": "^2.1.0",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vscode/webview-ui-toolkit": "^1.1.0",
    "get-port": "^6.1.2",
    "got": "^12.5.0",
    "lit": "^2.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "readable-stream": "^4.2.0",
    "svelte": "^3.50.1",
    "tmp-promise": "^3.0.3",
    "vercel": "^28.4.4",
    "vite": "^3.1.4",
    "vue": "^3.2.40",
    "xdg-app-paths": "^8.2.0",
    "yargs": "^17.5.1"
  }
}
